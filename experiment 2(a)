#include <stdio.h>
int main()
{
    int a,b,sum,diff,multiply,divide;
    printf("enter two numbers");
    scanf("%d%d",&a,&b);
    sum=a+b;
    printf("sum=%d",a+b);
    diff=a-b;
    printf("diff=%d",a-b);
    multiply=a*b;
    printf("multiply=%d",a*b);
    divide=a/b;
    printf("divide=%d",a/b);
    return 0;
}
