#include <stdio.h>
#include <math.h>
int main()
{
int x1,y1,x2,y2;
printf(“enter the co-ordinates of first point:\n”);
scanf(“%d%d”,&x1,&y1);
printf(“enter the co-ordinates of second point:\n”)
scanf(“%d%d”,&x2,&y2);
double d=sqrt(pow(x2-x1,2)+(pow(y2-y1,2)));
printf(“the distance of two points in a plane is:%1f”,d);
return 0;
}
